{
  "name": "cache-proxy-plus",
  "version": "v1.0.0",
  "description": "A typescript module template",
  "author": "Chakhsu.Lau",
  "license": "MIT",
  "keywords": [
    "npm",
    "github",
    "module",
    "typescript",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chakhsu/cache-proxy-plus"
  },
  "homepage": "https://github.com/chakhsu/cache-proxy-plus",
  "bugs": {
    "url": "https://github.com/chakhsu/cache-proxy-plus/issues"
  },
  "type": "module",
  "exports": "./lib/index.js",
  "types": "./lib/types/index.d.ts",
  "engines": {
    "node": ">=14.16"
  },
  "files": [
    "lib",
    "README_CN.md"
  ],
  "scripts": {
    "clear": "rimraf lib && rimraf coverage",
    "patch:esm-type": "echo '{ \"type\": \"module\" }' >> lib/package.json",
    "patch:esm-js": "tsc-esm-fix --tsconfig=tsconfig.json",
    "build:esm": "tsc -P tsconfig.json && pnpm patch:esm-js && pnpm patch:esm-type",
    "build": "pnpm clear && pnpm build:esm",
    "lint:prettier": "prettier --cache --check --ignore-path .gitignore --ignore-path .prettierignore .",
    "prettier": "pnpm lint:prettier --write",
    "prepare": "husky install",
    "test": "NODE_OPTIONS=--experimental-vm-modules pnpm exec jest"
  },
  "lint-staged": {
    "*.{ts,js,md,json,yaml}": "prettier --write"
  },
  "dependencies": {
    "joi": "^17.11.0",
    "lodash-es": "^4.17.21",
    "lru-cache": "^10.1.0",
    "p-queue": "^8.0.1",
    "p-timeout": "^6.1.2",
    "sync-defer": "^1.3.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.42",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.11",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.5",
    "bluebird": "^3.7.2",
    "debug": "^4.3.4",
    "husky": "^8.0.3",
    "ioredis": "^5.3.2",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsc-esm-fix": "^2.20.18",
    "typescript": "^5.3.3"
  }
}
